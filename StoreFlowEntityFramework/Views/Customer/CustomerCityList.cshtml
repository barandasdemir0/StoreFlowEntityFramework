@model List<string>
@{
    ViewData["Title"] = "CustomerCityList";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Müşteri Şehir Listesi</h4>
                <p class="card-description">
                    <code>Kategori İle İşlemlerinizi buradan yapabilirsiniz</code>
                </p>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                               
                                <th>Şehirler</th>


                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item</td>
             

                                </tr>
                            }


                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>

</div>

