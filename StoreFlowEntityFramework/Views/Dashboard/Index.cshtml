
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

       @await Component.InvokeAsync("_HeadDashboardComponentPartial")

<body>
    <div class="container-scroller">
   
        @await Component.InvokeAsync("_NavbarDashboardComponentPartial")
   
      
        <div class="container-fluid page-body-wrapper">
   
            @await Component.InvokeAsync("_ThemeSettingsWrapperDashboardComponentPartial")
            @await Component.InvokeAsync("_RightSidebarDashboardComponentPartial")
            @await Component.InvokeAsync("_SidebarDashboardComponentPartial")
    
          
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="page-header">
                        <h3 class="page-title">
                            Dashboard
                        </h3>
                    </div>
                    @await Component.InvokeAsync("_CardStatisticsDashboardComponentPartial")
                    @await Component.InvokeAsync("_ChartDashboardComponentPartial")
                    
                    <div class="row">
                        @await Component.InvokeAsync("_SalesStatusDashboardComponentPartial")
                        @await Component.InvokeAsync("_ActivityDashboardComponentPartial")
                        @await Component.InvokeAsync("_DailySalesDashboardComponentPartial")
                       

                       
                      
                    </div>
                   
                    @await Component.InvokeAsync("_Last5MessagesDashboardComponentPartial")


                    <div class="row">
                        

                        @await Component.InvokeAsync("_SalesDataDashboardComponentPartial")
                      
                       
                    </div>




                  

                    <div class="row">
                        @await Component.InvokeAsync("_TodoDashboardComponentPartial")
                        
                        @await Component.InvokeAsync("_Last5ProductDashboardComponentPartial")
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-body">
                                    <div class="d-md-flex justify-content-between align-items-center">
                                        <div class="d-flex align-items-center mb-3 mb-md-0">
                                            <button class="btn btn-social-icon btn-facebook btn-rounded">
                                                <i class="fab fa-facebook-f"></i>
                                            </button>
                                            <div class="ml-4">
                                                <h5 class="mb-0">Facebook</h5>
                                                <p class="mb-0">813 friends</p>
                                            </div>
                                        </div>
                                        <div class="d-flex align-items-center mb-3 mb-md-0">
                                            <button class="btn btn-social-icon btn-twitter btn-rounded">
                                                <i class="fab fa-twitter"></i>
                                            </button>
                                            <div class="ml-4">
                                                <h5 class="mb-0">Twitter</h5>
                                                <p class="mb-0">9000 followers</p>
                                            </div>
                                        </div>
                                        <div class="d-flex align-items-center mb-3 mb-md-0">
                                            <button class="btn btn-social-icon btn-google btn-rounded">
                                                <i class="fab fa-google-plus-g"></i>
                                            </button>
                                            <div class="ml-4">
                                                <h5 class="mb-0">Google plus</h5>
                                                <p class="mb-0">780 friends</p>
                                            </div>
                                        </div>
                                        <div class="d-flex align-items-center">
                                            <button class="btn btn-social-icon btn-linkedin btn-rounded">
                                                <i class="fab fa-linkedin-in"></i>
                                            </button>
                                            <div class="ml-4">
                                                <h5 class="mb-0">Linkedin</h5>
                                                <p class="mb-0">1090 connections</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © 2018. All rights reserved.</span>
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Hand-crafted & made with <i class="far fa-heart text-danger"></i></span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/vendors/js/vendor.bundle.addons.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/off-canvas.js"></script>
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/hoverable-collapse.js"></script>
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/misc.js"></script>
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/settings.js"></script>
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Melody-Premium-Bootstrap-Admin-Dashboard-Template/js/dashboard.js"></script>
    <!-- End custom js for this page-->
</body>


</html>
